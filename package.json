{
  "name": "nti-util-logger",
  "version": "1.1.2",
  "description": "Logger wrapper utility that will provide the .log .error .warn .info .debug methods",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=5.0.0",
    "npm": ">=3.3.0"
  },
  "scripts": {
    "test": "make test",
    "start": "make check",
    "prepublish": "not-in-install && make || in-install",
    "preversion": "make test",
    "postversion": "git push && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextThought/nti.util.logger.git"
  },
  "keywords": [
    "logger"
  ],
  "author": "nextthought",
  "license": "UNLICENCED",
  "dependencies": {
    "debug": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.1.0",
    "babel-eslint": "^7.0.0",
    "babel-jest": "^19.0.0",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-stage-1": "^6.16.0",
    "babel-register": "^6.18.0",
    "eslint-config-nti-codestyle-js": "^1.1.0",
    "in-publish": "^2.0.0",
    "jest": "^19.0.2",
    "jest-junit": "^1.4.0",
    "rollup": "^0.41.5",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^8.0.0",
    "rollup-plugin-json": "^2.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/*.spec.js"
    ],
    "coverageDirectory": "reports/coverage",
    "coverageReporters": ["text", "lcov", "cobertura"],
    "testResultsProcessor": "./node_modules/jest-junit"
  },
  "jest-junit": {
    "output": "reports/test-results/index.xml"
  }
}